[Desktop Entry]
X-AppInstall-Package=mess-desktop-entries
X-AppInstall-Popcon=0
X-AppInstall-Section=multiverse

Name=Game Boy Advance ROM Player
Comment=Play Nintendo Game Boy Advance games
Type=Application
Exec=/usr/games/mess gba -cartridge %f
Terminal=false
NoDisplay=true
MimeType=application/x-gba-rom;


X-Ubuntu-Gettext-Domain=app-install-data
