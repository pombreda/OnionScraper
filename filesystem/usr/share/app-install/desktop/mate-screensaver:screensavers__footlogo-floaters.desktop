[Desktop Entry]
X-AppInstall-Package=mate-screensaver
X-AppInstall-Popcon=324
X-AppInstall-Section=universe

Name=Floating MATE
Comment=Bubbles the MATE logo around the screen
Exec=/usr/lib/mate-screensaver/floaters /usr/share/pixmaps/mate-logo-white.svg
TryExec=/usr/lib/mate-screensaver/floaters
StartupNotify=false
Terminal=false
Type=Application
Categories=Screensaver;
Keywords=MATE;screensaver;logo;bubbles;
OnlyShowIn=MATE;

X-Ubuntu-Gettext-Domain=app-install-data
